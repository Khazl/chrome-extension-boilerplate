{
  "name": "Extension Boilerplate",
  "version": "1.0",
  "description": "Should do something useful...",
  "permissions": ["http://*/*", "https://*/*", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
     "default_popup": "popup.html",
     "default_title": "Extension Boilerplate",
     "default_icon": {
       "16": "images/16x16.png",
       "32": "images/32x32.png",
       "48": "images/48x48.png",
       "128": "images/128x128.png"
     }
  },
  "icons": {
    "16": "images/16x16.png",
    "32": "images/32x32.png",
    "48": "images/48x48.png",
    "128": "images/128x128.png"
  },
  "manifest_version": 2,
  "update_url": "https://clients2.google.com/service/update2/crx"
}
